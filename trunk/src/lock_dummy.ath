context: pconf *
context: cconf *

brick #lock_dummy     

purpose Grant any lock to anyone

desc
	Used in #lock_mand as "lock manager" for the internally used #lock_cache.
enddesc

output :>out

	operation $lock,$unlock {
		@success = TRUE;
	}
	
	operation $op {
		@success = FALSE;
	}
